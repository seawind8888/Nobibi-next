{
  "name": "Nobibi-next",
  "version": "1.0.0",
  "description": "A Next.js appliction",
  "main": "server.js",
  "author": "seawind8888",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "start:pm2": "NODE_ENV=production next build&& pm2 start pm2.config.js",
    "build": "next build",
    "prod": "NODE_ENV=production node server.js",
    "lint:fix": "eslint --fix"
  },
  "keywords": [
    "nextjs",
    "antd",
    "react",
    "ssr",
    "redux",
    "redux-saga"
  ],
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^3.8.4",
    "babel-plugin-import": "^1.9.0",
    "dotenv": "^8.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "if-comp": "^0.0.8",
    "isomorphic-unfetch": "^3.0.0",
    "less": "^3.8.1",
    "less-vars-to-js": "^1.3.0",
    "lodash": "^4.17.19",
    "md5": "^2.2.1",
    "next": "latest",
    "next-redux-saga": "^3.0.0",
    "next-redux-wrapper": "^2.0.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-no-ssr": "^1.1.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "braft-editor": "^2.3.7",
    "eslint": "^5.4.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "js-cookie": "^2.2.0",
    "lint-staged": "^8.1.0",
    "moment": "^2.24.0",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.5",
    "terser-webpack-plugin": "^1.1.0",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
